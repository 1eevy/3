Nx := 128
Ny := 64
setgridsize(Nx, Ny, 1)
setcellsize(3e-9, 3e-9, 3e-9)

Msat = 800e6
Aex = 13e-12
alpha = 0.02
m = uniform(1, 0, 0)

bias := 0.05
B_ext = vector(bias, 0, 0)
relax()


BW := 3e9
maxDt = 1/(2*BW)
t0 := 5e-9
A := 5e-3 * 1e-9 // impulse in T*s
B_ext = vector(bias, (A*BW)*sinc((t-t0)*2*pi*BW), 0)

tableAdd(B_ext)
tableAutoSave(1/(10*BW))
run(50e-9)
